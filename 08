#include <stdio.h>

int main() {
	float temperaturaf,temperaturac
	
	
	printf("Digite a temperatura em Fahrenheit: ";
	scanaf("%f"&temperaturaf);
	
	temperaturac = (temperaturaf - 32) / 1.8;
	
	printf("Temperatura em Celseius: %.2f\n",temperaturac);
	
	printf("Temperatura em Fahrenheit: %.2f\n",temperaturaf);
	
	
	
	return 0;
}
