# include <stdio.h>
#include <stdlib.h>

int main(){
    float comprimento,largula,area;
   	
   	
   	printf("Digite o comprimento do triangulo: ");
   	scanf("%f",&comprimento);
   	
   	printf("Digite a largula: ");
   	scanf("%f",&largula);
   	
  
   	area = comprimento * largula;
   	
   	printf("A area do triangulo e: %.2f",area);
   	

   	return 0;	

}
